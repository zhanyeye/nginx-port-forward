
upstream book_pool{
    server 127.0.0.1:8888;
}

#Demo1端口转发
server {
    listen       80;
    server_name  jupyter.zhanyeye.com;
    
    #将所有请求转发给demo_pool池的应用处理
    location / {
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_pass http://127.0.0.1:8888;
    }
}
